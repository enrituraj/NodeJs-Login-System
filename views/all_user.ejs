<header class="navbar navbar-expand-md navbar-light d-print-none">
    <div class="container-xl">
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu"
        aria-controls="navbar-menu" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
        LoginApp
      </h1>
      <div class="navbar-nav flex-row order-md-last">
        <div class="nav-item dropdown">
          <a href="#" class="nav-link d-flex lh-1 text-reset p-0" data-bs-toggle="dropdown" aria-label="Open user menu">
            <% if(typeof user.img !='undefined' ){ %>
              <span class="avatar avatar-sm" style="background-image: url(./static/avatars/003m.jpg)"></span>
              <%}else{ %>
                <span class="avatar avatar-sm">
                  <%= typeof user.name !='undefined' ? user.name[0] : 'N/A' %>
                </span>
                <% } %>
  
                  <div class="d-none d-xl-block ps-2">
                    <div>
                      <%= user.name %>
                    </div>
                    <div class="mt-1 small text-muted">
                      <%= user.email %>
                    </div>
                  </div>
          </a>
          <div class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
            <a href="#" class="dropdown-item">Status</a>
            <a href="/edit-profile" class="dropdown-item">Edit Profile</a>
            <a href="#" class="dropdown-item">Feedback</a>
            <a href="" class="dropdown-item">Settings</a>
            <div class="dropdown-divider"></div>
            <form class="dropdown-item" action="/logout?_method=DELETE" method="post">
              <button type="submit" class="btn btn-danger w-100">
                <svg xmlns="http://www.w3.org/2000/svg" class="icon dropdown-item-icon" width="24" height="24"
                  viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
                  stroke-linejoin="round">
                  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                  <path d="M14 8v-2a2 2 0 0 0 -2 -2h-7a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h7a2 2 0 0 0 2 -2v-2"></path>
                  <path d="M7 12h14l-3 -3m0 6l3 -3"></path>
                </svg>
                Logout
              </button>
            </form>
          </div>
        </div>
      </div>
      <div class="collapse navbar-collapse" id="navbar-menu">
        <div class="d-flex flex-column flex-md-row flex-fill align-items-stretch align-items-md-center">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link" href="/dashboard">
                <span class="nav-link-icon d-md-none d-lg-inline-block">
                  <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                    stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                    <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                    <path
                      d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z">
                    </path>
                  </svg>
                </span>
                <span class="nav-link-title">
                  Dashboard
                </span>
              </a>
            </li>
            <% if(user.role =='Admin' ){ %>
              <li class="nav-item">
                <a class="nav-link" href="/admin">
                  <span class="nav-link-icon d-md-none d-lg-inline-block">
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                      stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                      <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                      <path
                        d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z">
                      </path>
                    </svg>
                  </span>
                  <span class="nav-link-title">
                    Admin Dashboard
                  </span>
                </a>
              </li>
              <li class="nav-item active">
                <a class="nav-link" href="/all_user">
                  <span class="nav-link-icon d-md-none d-lg-inline-block">
                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24"
                      stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                      <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                      <path
                        d="M12 17.75l-6.172 3.245l1.179 -6.873l-5 -4.867l6.9 -1l3.086 -6.253l3.086 6.253l6.9 1l-5 4.867l1.179 6.873z">
                      </path>
                    </svg>
                  </span>
                  <span class="nav-link-title">
                    All User
                  </span>
                </a>
              </li>
            <%}%>
          </ul>
        </div>
      </div>
    </div>
  </header>
  
  
  <div class="page-wrapper">
    <!-- Page header -->
    <div class="page-header d-print-none">
      <div class="container-xl">
        <div class="row g-2 align-items-center">
            <div class="col">
              <h2 class="page-title">
                All User
              </h2>
            </div>
            <!-- Page title actions -->
            <div class="col-auto ms-auto d-print-none">
              <a href="#" class="btn btn-primary">
                <!-- Download SVG icon from http://tabler-icons.io/i/plus -->
                <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round"><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
                Table View
              </a>
            </div>
          </div>
      </div>
    </div>
    <!-- Page body -->
    <div class="page-body">
      <div class="container-xl">
  
        <div class="row row-cards">
        
            <% all_user.forEach(user=> { %>
                <div class="col-md-6 col-xl-3">
                    <div class="card">
                      <div class="card-body text-center">
                        <div class="mb-3">
                          <% if(typeof user.img != 'undefined'){ %>
                            <span class="avatar avatar-xl avatar-rounded" style="background-image: url(./static/avatars/003m.jpg)"></span>
                            <%}else{ %>
                            <span class="avatar avatar-xl avatar-rounded">
                              <%= typeof user.name != 'undefined' ? user.name[0] : 'N/A' %>
                            </span>
                            <% } %>
                        </div>
                        <div class="card-title mb-1"><%= typeof user.name !='undefined' ? user.name : 'N/A' %></div>
                        <div class="text-muted">                            
                            <% if(user.role=='Admin' ){ %>
                                <span class="badge bg-green-lt">Admin</span>
                            <%}else{ %>
                                <span class="badge bg-red-lt">User</span>
                            <% } %>
                        </div>
                      </div>
                      <a href="#" class="card-btn">View full profile</a>
                    </div>
                </div>
                <% }) %>
        </div>
        
        <div class="row row-cards">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">All User</h3>
              </div>
              <div class="table-responsive">
                <table class="table card-table table-vcenter text-nowrap datatable">
                  <thead>
                    <tr>
                      <th class="w-1">No.
                        <!-- Download SVG icon from http://tabler-icons.io/i/chevron-up -->
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-sm icon-thick" width="24" height="24"
                          viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
                          stroke-linejoin="round">
                          <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                          <polyline points="6 15 12 9 18 15"></polyline>
                        </svg>
                      </th>
                      <th>Name</th>
                      <th>Email</th>
                      <th>Phone no</th>
                      <th>Role</th>
                      <th>Action</th>
                    </tr>
                  </thead>
  
                  <tbody>
  
  
                    <% all_user.forEach(user=> { %>
                      <tr>
                        <th>
                          <%= user.no %>
                        </th>
                        <td>
                          <%= typeof user.name !='undefined' ? user.name : 'N/A' %>
                        </td>
                        <td>
                          <%= typeof user.email !='undefined' ? user.email : 'N/A' %>
                        </td>
                        <td>
                          <%= typeof user.phone !='undefined' ? user.phone : 'N/A' %>
                        </td>
                        <td>
                          <% if(user.role=='Admin' ){ %>
                            <span class="badge bg-green-lt">Admin</span>
                            <%}else{ %>
                              <span class="badge bg-red-lt">User</span>
                              <% } %>
                        </td>
                        <td>
                          <div class="btn-list flex-nowrap">
                            <div class="dropdown">
                              <a href="#" class="btn-action dropdown-toggle" data-bs-toggle="dropdown"
                                aria-haspopup="true" aria-expanded="false">
                                <!-- Download SVG icon from http://tabler-icons.io/i/dots-vertical -->
                                <svg xmlns="http://www.w3.org/2000/svg" class="icon" width="24" height="24"
                                  viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none"
                                  stroke-linecap="round" stroke-linejoin="round">
                                  <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                  <circle cx="12" cy="12" r="1"></circle>
                                  <circle cx="12" cy="19" r="1"></circle>
                                  <circle cx="12" cy="5" r="1"></circle>
                                </svg>
                              </a>
                              <div class="dropdown-menu dropdown-menu-end">

                                <a class="dropdown-item" href="/view_user">View User</a>
                                <a class="dropdown-item" href="#" data-bs-toggle="modal"
                                  data-bs-target="#modal-report<%= user.no%>">Edit User</a>
                                  <form action="/delete_user/<%= user._id%>?_method=DELETE" method="post">
                                  <button type="submit" class="dropdown-item">             
                                    Delete User
                                  </button>
                                </form>
                              
                              
                              </div>
                            </div>
                          </div>
                        </td>
                      </tr>
  
  
  
                      <div class="modal modal-blur fade" id="modal-report<%= user.no%>" tabindex="-1"
                        style="display: none;" aria-hidden="true">
                        <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <h5 class="modal-title">Edit User</h5>
                              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <form action="/edit_profile?_method=PUT" method="post">
                              <div class="modal-body">
  
                                <div class="row">
                                  <div class="col-sm-6 col-md-6">
                                    <div class="mb-3">
                                      <label class="form-label">Email address</label>
                                      <div>
                                        <input type="email" readonly name="email" class="form-control"
                                          value="<%= typeof user.email != 'undefined' ? user.email : 'N/A' %>">
                                        <small class="form-hint">We'll never share your email with anyone else.</small>
                                      </div>
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Full Name</label>
                                      <input type="text" class="form-control"
                                        value="<%= typeof user.name != 'undefined' ? user.name : 'N/A' %>"
                                        placeholder="Enter Your Full Name" name="name">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Username</label>
                                      <input type="text" class="form-control"
                                        value="<%= typeof user.username != 'undefined' ? user.username : 'N/A' %>"
                                        placeholder="Enter Your Username" name="username">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
                                    <div class="mb-3">
                                      <label class="form-label">Phone No</label>
                                      <input type="number" class="form-control"
                                        value="<%= typeof user.phone != 'undefined' ? user.phone : 'N/A' %>"
                                        placeholder="Enter Your Phone No" name="phone">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Dob</label>
                                      <input type="date" class="form-control"
                                        value="<%= typeof user.dob != 'undefined' ? user.dob : '01/01/1900' %>"
                                        placeholder="Enter Your Dob" name="dob">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Address</label>
                                      <input type="text" class="form-control"
                                        value="<%= typeof user.address != 'undefined' ? user.address : 'N/A' %>"
                                        placeholder="Enter Your Address" name="address">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Pincode</label>
                                      <input type="number" class="form-control"
                                        value="<%= typeof user.pincode != 'undefined' ? user.pincode : '0' %>"
                                        placeholder="Enter Your Pincode" name="pincode">
                                    </div>
                                  </div>
                                  <div class="col-sm-6 col-md-6">
  
                                    <div class="mb-3">
                                      <label class="form-label">Country</label>
                                      <input type="text" class="form-control"
                                        value="<%= typeof user.country != 'undefined' ? user.country : 'N/A' %>"
                                        placeholder="Enter Your Country" name="country">
                                    </div>
                                  </div>
                                </div>
                              </div>
                              <div class="card-footer text-end">
                                <div class="d-flex">
                                  <a href="#" class="btn btn-link link-secondary" data-bs-dismiss="modal">
                                    Cancel
                                  </a>
                                  <button type="submit" class="btn btn-primary ms-auto">Update Now</button>
                                </div>
                              </div>
                            </form>
                          </div>
                        </div>
                      </div>
  
  
                      <% }) %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
  
  
  
  
  
  
  
        </div>
      </div>
    </div>
  </div>